header{
    display:none
}
.container-fluid section div.row>div {
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    row-gap:40px;
    column-gap:30px;
}


.p2g_6pmcU.q2g_6pmcU>div {
    min-width: 100% !important;
    padding:0px !important;
    margin:0px !important;
}

.card-body>p {
    display: none;
}

.card .card-body div:nth-of-type(2) button {
    width: 144px;
 }

.flex.flex-wrap {
    display: none;
}

.card-body>div:nth-of-type(1) {
    display: none;
}

.card-body {
    display: flex;
    align-items: baseline;
    justify-content: space-between;
    background-color: #f2f2f2;
}

@media (max-width:1439px){
    .container-fluid section div.row>div {
            grid-template-columns:1fr 1fr 1fr;
}
}

@media (max-width:1024px){
    .container-fluid section div.row>div {
            grid-template-columns:1fr 1fr;
    }
}

@media (max-width:766px){
    .container-fluid section div.row>div {
            grid-template-columns:1fr;
    }
}

.card .card-body div:nth-of-type(2) button{
     width: 124px;
}

@media (max-width:374px){
    .card .card-body div:nth-of-type(2) button{
     width: 80px;
    }
}



.sticky-inner-wrapper section div div {
    width: 78px;
    height: 78px;
    min-width: 78px;
    border: 3px dashed #bcb6b6;
    background-color: #f2f2f2;
}


.container-fluid section {
    background-color: #f2f2f2;
}

.card-title{
    font-size:16px;
}

.card>div p {
    display: none;
}

.container-fluid>div:nth-of-type(3) button {
    display: none;
}


.LazyLoad button {
    display: none !important;
}

.container-fluid>div:nth-of-type(2)>button {
    display: none!important;
}

.container-fluid>div:nth-of-type(2)>div {
    display: none!important;
}
